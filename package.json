{
	"name": "jotref",
	"version": "0.1.0",
	"description": "Jot your references in CLI",
	"keywords": [
		"cli",
		"list",
		"notes",
		"dev"
	],
	"homepage": "https://github.com/ParikshithV/CLIst#readme",
	"bugs": {
		"url": "https://github.com/ParikshithV/CLIst/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ParikshithV/CLIst.git"
	},
	"license": "MIT",
	"author": "Parikshith V",
	"type": "module",
	"main": "index.js",
	"bin": {
		"clist": "dist/cli.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"buildrun": "tsc && node dist/cli.js"
	},
	"dependencies": {
		"clipboardy": "^5.0.2",
		"fullscreen-ink": "^0.1.0",
		"ink": "^4.1.0",
		"ink-text-input": "^5.0.0",
		"lowdb": "^7.0.1",
		"meow": "^11.0.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"engines": {
		"node": ">=16"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
